<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">
<changeSet id="0004" author="Mariusz">
    <createTable tableName="system_requirements">
        <column name="id" type="BIGINT" autoIncrement="true">
            <constraints nullable="false" unique="true" primaryKey="false"/>
        </column>
        <column name="system" type="VARCHAR(100)">
            <constraints nullable="true"/>
        </column>
        <column name="requirements" type="VARCHAR(5000)">
            <constraints nullable="false"/>
        </column>

    </createTable>
</changeSet>
</databaseChangeLog>