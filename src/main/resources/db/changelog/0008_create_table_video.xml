<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">
<changeSet id="0008" author="Mariusz">
    <createTable tableName="video">
        <column name="id" type="BIGINT" autoIncrement="true">
            <constraints nullable="false" unique="true" primaryKey="false"/>
        </column>
        <column name="url" type="VARCHAR(2000)">
            <constraints nullable="false"/>
        </column>
        <column name="product_id" type="BIGINT">
            <constraints foreignKeyName="fk_product_2" referencedTableName="product"
                         referencedColumnNames="id" nullable="false"/>
        </column>
    </createTable>
</changeSet>
</databaseChangeLog>